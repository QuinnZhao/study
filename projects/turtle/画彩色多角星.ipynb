    "from turtle import *\n",
    "\n",
    "speed(0)\n",
    "color('red', 'yellow')\n",
    "hideturtle()\n",
    "begin_fill()\n",
    "while True:\n",
    "    forward(100)\n",
    "    left(170)\n",
    "    if abs(position()) < 1:\n",
    "        break\n",
    "end_fill()\n",
    "mainloop()"
