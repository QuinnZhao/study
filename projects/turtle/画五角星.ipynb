   "source": [
    "# 画紫色的五角星\n",
    "\n",
    "import turtle\n",
    "\n",
    "turtle.color(\"purple\")\n",
    "turtle.pensize(5)\n",
    "turtle.goto(0,0)\n",
    "turtle.speed('fast')\n",
    "for i in range(6):\n",
    "    turtle.forward(100)\n",
    "    turtle.right(144)\n",
    "turtle.up()\n",
    "turtle.goto(-150,-120)\n",
    "turtle.color(\"red\")\n",
    "turtle.write(\"Done\")\n",
    "\n",
    "turtle.mainloop()"
   ]
  }
 ],
